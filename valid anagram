import java.util.Arrays;

class Solution {
    public boolean isAnagram(String s, String t) {
        char[] ch=s.toCharArray();
        char[] rh=t.toCharArray();
       
        Arrays.sort(ch);
         System.out.println(ch);
        Arrays.sort(rh);
         System.out.println(rh);
        return Arrays.equals(ch,rh);
       


    }
}
